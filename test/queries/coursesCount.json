{
    "title": "SELECT dept, avg WHERE avg > 97",
    "query": {
        "WHERE": {
            "OR": [
                {
                    "AND": [
                        {
                            "GT": {
                                "courses_avg": 90
                            }
                        },
                        {
                            "IS": {
                                "courses_dept": "adhe"
                            }
                        }
                    ]
                },
                {
                    "EQ": {
                        "courses_avg": 97
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_title",
                "count"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "count"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_title"
            ],
            "APPLY": [
                {
                    "count": {
                        "COUNT": "courses_pass"
                    }
                }
            ]
        }
    },
